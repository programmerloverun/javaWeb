<?xml version="1.0" encoding="UTF-8" ?>
<!--
    属性
    1.属性值用双引号或者单引号分隔
    2.一个元素可以有多个属性 基本格式：<student id="100" name="John Doe" age>
    3.特定的属性名称在同一个元素标记中只能出现一次
    4.属性值不能出现& < > ' " 这些特殊字符
-->
<students>
    <student id="100">
        <name>John Doe</name>
        <age>20</age>
        <gender>男</gender>
    </student>
    <student id="101">
        <name>gege</name>
        <age>17</age>
        <gender>女</gender>
    </student>
    <school>大连东软信息学院</school>
    <address>大连市甘井子区</address>
    <city/>
</students>